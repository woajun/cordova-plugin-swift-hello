<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.test.sayHello" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>sayHelloPlugin</name>
    <js-module name="sayHelloPlugin" src="www/sayHelloPlugin.js">
        <clobbers target="cordova.plugins.sayHelloPlugin" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="sayHelloPlugin">
                <param name="ios-package" value="sayHelloPlugin" />
            </feature>
        </config-file>
        <source-file src="src/ios/sayHelloPlugin.m" />
    </platform>
</plugin>